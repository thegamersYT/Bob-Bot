module.exports = ({
 name: "clear",
 code: `
$onlyPerms[managemessages;need "managemessages" permission]
$clear[$message] **$message messages deleted correctly**
$onlyIf[$getGlobalUserVar[Blacklist;$authorID]==false;**⛔ You are blacklisted**]` 
}) 

